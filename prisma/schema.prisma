// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Listing {
    id          String   @id
    title       String
    link        String
    location    String?
    description String?
    company     String
    createdAt   DateTime @default(now())
    valid       Boolean  @default(true)
}
